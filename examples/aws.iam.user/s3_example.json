{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                "s3:Get*",
                "s3:Create*",
                "s3:List*",
                "s3:Put*"
            ],

            "Resource": [
                "arn:aws:s3:::example.bucket.com*/*",
                "arn:aws:s3:::example.bucket.com*"
            ]
        }
    ]
}
